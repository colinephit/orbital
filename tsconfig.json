{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/toDoList/components/HomePageButton.tsx", "app/toDoList/components/HomePageButton.tsx", "app/Login.test.js", "app/Login.test.js", "tests/communitypage.test.js", "tests/timer.test.js", "tests/timer.test.js"],
  "exclude": [
    "node_modules"
  ]
}
