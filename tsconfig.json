{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "app/toDoList/components/HomePageButton.tsx", "app/toDoList/components/HomePageButton.tsx", "firebase/clientApp.ts",
  "exapp/components/toDoList/components/HomePageButton.tsx",
    "app/components/toDoList/components/HomePageButton.tsx"
  ]
}
